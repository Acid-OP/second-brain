{
  "version": 2,
  "builds": [
    {
      "src": "Client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "zeroConfig": false,
        "distDir": "Client/dist",
        "buildCommand": "cd Client && npm install && npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Client/dist/$1"
    }
  ]
}